{
  "name": "mitranim.github.io",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run bundle && gulp server",
    "gulp": "gulp",
    "img": "gulp images",
    "bundle": "jspm bundle angular + lodash + firebase + fireproof",
    "bundle-prod": "jspm bundle boot --minify --no-mangle",
    "bundle-sfx": "jspm bundle-sfx boot",
    "bundle-sfx-prod": "jspm bundle-sfx boot --minify --no-mangle",
    "build-prod": "env GULP_BUILD_TYPE=production gulp build && npm run bundle-sfx-prod",
    "serve-prod": "npm run build-prod && gulp bsync",
    "push": "npm run build-prod && (cd mitranim-master && git add -A . && git commit -a -m autocommit && git push origin master)"
  },
  "authors": [
    "Mitranim <me@mitranim.com>"
  ],
  "homepage": "http://mitranim.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mitranim/mitranim.github.io.git"
  },
  "devDependencies": {
    "browser-sync": "^2.7.1",
    "cheerio": "^0.19.0",
    "gulp": "git://github.com/gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^2.2.0",
    "gulp-babel": "^5.1.0",
    "gulp-concat": "^2.5.2",
    "gulp-filter": "^2.0.2",
    "gulp-if": "^1.2.5",
    "gulp-image-resize": "^0.6.0",
    "gulp-less": "^3.0.3",
    "gulp-load-plugins": "^0.10.0",
    "gulp-marked": "^1.0.0",
    "gulp-minify-css": "^1.1.1",
    "gulp-minify-html": "^1.0.2",
    "gulp-ng-html2js": "^0.2.0",
    "gulp-plumber": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rimraf": "^0.1.1",
    "gulp-statil": "^0.0.2",
    "gulp-typescript": "^2.7.3",
    "gulp-watch": "^4.2.4",
    "highlight.js": "^8.5.0",
    "jspm": "^0.15.6",
    "lodash": "^3.8.0",
    "tsd": "^0.6.0",
    "typescript": "Microsoft/TypeScript#681954bc59a58da949de42272333b3b33f72bce9"
  },
  "jspm": {
    "directories": {
      "baseURL": "mitranim-master"
    },
    "configFile": "system.config.js",
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.3.15",
      "angular-sanitize": "github:angular/bower-angular-sanitize@^1.3.15",
      "firebase": "github:firebase/firebase-bower@^2.2.4",
      "fireproof": "npm:fireproof@^2.5.2",
      "foliant": "npm:foliant@^0.0.1",
      "lodash": "npm:lodash@^3.8.0",
      "ng-decorate": "npm:ng-decorate@^0.0.7"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
      "babel-runtime": "npm:babel-runtime@^5.1.13",
      "core-js": "npm:core-js@^0.9.4"
    }
  }
}
